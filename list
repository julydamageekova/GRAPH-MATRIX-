#include <iostream>
using namespace std;

int Add(int* terminal, int* next_el, int* head, int k, int v, int u){
    if(!head[v])
        head[v] = 0;
    k = k + 1;
    terminal[k] = u;
    next_el[k] = head[v];
    head[v] = k;
    return k;
}


int main(){
    int k = 0, n, m, v, u, i, r, j;
    cout <<" Number of vertices "; cin >> n;
    cout <<" Number of edges "; cin >> m;
    cout <<" Incidence edges" << endl;
    int terminal[100];
    int next_el[1000];
    int head[1000];
    for (i = 0; i < m; i++){
        cin >> v >> u;
        k = Add(terminal, next_el, head, k, v, u);
        k = Add(terminal, next_el, head, k, u, v);
    }
    for (i = 0; i < n; i++){
        j = head[i];
        if (i){
            cout<<i<<" : ";
            while (j > 0){
                if (!next_el[j])
                    cout << terminal[j];
                else
                    cout << terminal[j]<<",";
                j = next_el[j];
            }
        }
        cout<<endl;
    }
    return 0;
}
